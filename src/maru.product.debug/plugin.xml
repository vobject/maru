<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="maru.application"
            name="Maru (Debug)">
         <property
               name="appName"
               value="Maru (Debug)">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="aboutImage"
               value="about.gif">
         </property>
         <property
               name="aboutText"
               value="Maru (Debug)&#x0A;&#x0A;Version 1.1.0&#x0A;License: Apache License, Version 2.0&#x0A;Visit: https://github.com/vobject/maru&#x0A;&#x0A;This product includes software and media&#x0A;developed by the following third parties:&#x0A;- Orekit: https://www.orekit.org&#x0A;- Apache Commons Math: http://commons.apache.org/proper/commons-math&#x0A;- JOGL: https://jogamp.org/jogl&#x0A;- NASA World Wind: http://worldwind.arc.nasa.gov&#x0A;- Oxygen Team: http://www.oxygen-icons.org&#x0A;- Unrestrictedstock.com: http://unrestrictedstock.com">
         </property>
         <property
               name="windowImages"
               value="icons/maru_16.png,icons/maru_32.png,icons/maru_48.png,icons/maru_64.png,icons/maru_128.png">
         </property>
         <property
               name="startupForegroundColor"
               value="FFFFFF">
         </property>
         <property
               name="startupMessageRect"
               value="7,245,445,20">
         </property>
         <property
               name="startupProgressRect"
               value="5,270,445,15">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.splashHandlers">
      <splashHandlerProductBinding
            productId="maru.product.debug.product"
            splashId="maru.product.debug.splashHandler">
      </splashHandlerProductBinding>
   </extension>

</plugin>
